{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Demo project for SE-2223 and SE-2226: AY-2021-2022: frontend.",
  "productName": "Sophies Demo",
  "author": "Richard Michael Coo <richard.michael.coo@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage",
    "badges": "istanbul-badges-readme --statementsLabel='Frontend Code Coverage' --logo=jest",
    "badges:ci": "pnpm badges --ci --exitCode=1",
    "start:dev": "quasar dev",
    "build": "pnpm typecheck && quasar build",
    "typecheck": "vue-tsc --noEmit",
    "deploy": "pnpm install && pnpm build"
  },
  "dependencies": {
    "@quasar/extras": "^1.0.0",
    "axios": "^0.27.2",
    "backend": "workspace:^0.0.1",
    "dotenv": "^16.0.1",
    "localforage": "^1.10.0",
    "quasar": "^2.6.0",
    "vite": "^2.9.9",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.0-alpha.0",
    "@quasar/vite-plugin": "^1.0.10",
    "@types/node": "^12.20.52",
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.3",
    "happy-dom": "^4.0.1",
    "istanbul-badges-readme": "^1.8.1",
    "postcss": "^8.4.13",
    "prettier": "^2.5.1",
    "rollup": "^2.72.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "vite-plugin-commonjs": "^0.4.7",
    "vitest": "^0.12.9",
    "vue-tsc": "^0.34.11"
  },
  "engines": {
    "node": "^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
